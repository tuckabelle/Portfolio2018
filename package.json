{
  "name": "frasco",
  "version": "0.8.4",
  "description": "Jekyll starter project including full setup for gulp, Sass, Autoprefixer, PostCSS, webpack, imagemin, Browsersync etc.",
  "main": "gulpfile.js",
  "scripts": {
    "preinstall": "bundle install --path vendor/bundle",
    "start": "bundle exec gulp",
    "test": "bundle exec gulp test",
    "browsersync": "bundle exec gulp browsersync",
    "browsersync-production": "bundle exec gulp browsersync --jekyllEnv='production'",
    "build": "bundle exec gulp build --jekyllEnv='production'",
    "eslint": "bundle exec gulp eslint",
    "imagemin": "bundle exec gulp imagemin",
    "production": "bundle exec gulp --jekyllEnv='production'",
    "sass": "bundle exec gulp sass",
    "stylelint": "bundle exec gulp styleLint",
    "stylelint-fix": "bundle exec gulp styleLint --fix",
    "watch": "bundle exec gulp watch"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Autoprefixer",
    "Browsersync",
    "gulp",
    "imagemin",
    "Jekyll",
    "PostCSS",
    "Sass",
    "Webpack"
  ],
  "author": "Kite",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ixkaito/frasco/issues"
  },
  "homepage": "https://ixkaito.github.io/frasco",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.4.4",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.13",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.7",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-stylelint": "^8.0.0",
    "gulp-watch": "^5.0.1",
    "imagemin-pngquant": "^6.0.0",
    "postcss-loader": "^7.0.2",
    "postcss-object-fit-images": "^1.1.2",
    "prettier-stylelint": "^0.4.2",
    "require-dir": "^1.1.0",
    "prettier": "^2.1.1",
    "sass": "^1.26.10",
    "stylelint": "^13.7.0",
    "stylelint-config-sass-guidelines": "^7.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.17.1",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-stream": "^7.0.0",
    "yargs": "^12.0.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.2.0",
    "@barba/core": "^2.1.1",
    "@barba/prefetch": "^2.1.6",
    "animejs": "^3.0.1",
    "barba.js": "^1.0.0",
    "gsap": "^2.0.2",
    "lottie-web": "^5.7.3",
    "masonry-layout": "^4.2.2",
    "rfs": "^8.0.4",
    "smooth-scroll": "^15.0.0"
  }
}
